    <style name="Animation.Activity.Default" parent="@android:style/Animation.Activity">
        <!-- Activity A跳转到Activity B时, Activity B进入动画 -->
        <item name="android:activityOpenEnterAnimation">@anim/push_left_in</item>
        <!-- Activity A跳转到Activity B时, Activity A退出动画 -->
        <item name="android:activityOpenExitAnimation">@anim/push_left_out</item>
        <!-- Activity B返回到Activity A时, Activity A进入动画 -->
        <item name="android:activityCloseEnterAnimation">@anim/base_slide_right_in</item>
        <!-- Activity B返回到Activity A时, Activity B退出动画 -->
        <item name="android:activityCloseExitAnimation">@anim/base_slide_right_out</item>
    </style>
